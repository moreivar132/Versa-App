{
  "name": "versa-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "printf \"/api/*  %s/api/:splat  200\\n/uploads/*  %s/uploads/:splat  200\\n/*  /index.html  200\" \"$VITE_API_URL\" \"$VITE_API_URL\" > public/_redirects",
    "build": "vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "guard:api": "bash scripts/guard-api.sh",
    "finsaas:check": "node scripts/finsaas-check-api.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "e2e": "playwright test"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "eslint": "^9.39.2",
    "globals": "^14.0.0",
    "vite": "^5.4.21",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "quagga": "^0.12.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}